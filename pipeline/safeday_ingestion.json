{
	"name": "safeday_ingestion",
	"properties": {
		"activities": [
			{
				"name": "Copy_from_blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "id",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"name": "level",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "level"
								}
							},
							{
								"source": {
									"name": "docNo",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "docNo"
								}
							},
							{
								"source": {
									"name": "variant",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "variant"
								}
							},
							{
								"source": {
									"name": "workArea",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "workArea"
								}
							},
							{
								"source": {
									"name": "equipment",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "equipment"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SafeDay_raw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "hse_sql_db",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}